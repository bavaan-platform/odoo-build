<?xml version="1.0"?>
<data inherit_id="stock.report_picking" priority="50">

	<xpath expr="//table[4]/thead/tr/th[4]" position="replace">
	</xpath>
	<xpath expr="//table[4]/thead/tr/th[5]" position="after">
		<th>
			<strong>Quantity</strong>
		</th>
	</xpath>

	<xpath expr="//table[4]/tbody/t/t/tr/td[3]" position="replace">
	</xpath>

	<xpath expr="//table[4]/tbody/t/t/tr/td[5]" position="after">
		<td>
			<span t-if="o.state != 'done'" t-field="ml.product_uom_qty"/>
			<span t-if="o.state == 'done'" t-field="ml.qty_done"/>
			<span t-field="ml.product_uom_id" groups="uom.group_uom"/>
		</td>
	</xpath>

</data>